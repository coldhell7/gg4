<p id="notice"><%= notice %></p>

<h1>Listing Gusers</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gusers.each do |guser| %>
      <tr>
        <td><%= guser.email %></td>
        <td><%= guser.password %></td>
        <td><%= link_to 'Show', guser %></td>
        <td><%= link_to 'Edit', edit_guser_path(guser) %></td>
        <td><%= link_to 'Destroy', guser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Guser', new_guser_path %>
